{
  "version": 2,
  "name": "neuralnet",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "^/favicon.png",
      "dest": "/favicon.png"
    },
    {
      "src": "^/script.min.js",
      "dest": "/script.min.js"
    },
    {
      "src": "^/style.css",
      "dest": "/style.css"
    }
  ]
}